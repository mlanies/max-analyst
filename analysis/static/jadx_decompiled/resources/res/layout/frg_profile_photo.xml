<?xml version="1.0" encoding="utf-8"?>
<ru.ok.messages.views.widgets.SlideOutLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/black"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/frg_profile_photo__progress"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"/>
        <one.me.sdk.zoom.ZoomableDraweeView
            android:id="@+id/frg_profile_photo__iv_photo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:adjustViewBounds="true"
            android:layout_centerInParent="true"/>
        <ru.ok.messages.views.widgets.BlockedAttachView
            android:id="@+id/frg_unknown_attach__deleted_view"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </RelativeLayout>
</ru.ok.messages.views.widgets.SlideOutLayout>
